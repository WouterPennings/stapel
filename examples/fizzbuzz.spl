inline STDOUT       1 end

; INPUT  [ len addr ]
; OUTPUT [ ]
proc print do
    STDOUT 1 syscall4 pop
end


proc main do
    1 while dup 100 < do
    
        if dup 15 % 0 =  do
            "FizzBuzz\n" print

        elif dup 3 % 0 = do
            "Fizz\n"     print

        elif dup 5 % 0 = do
            "Buzz\n"     print

        else 
            dup put

        end

        1 +
    end
end 